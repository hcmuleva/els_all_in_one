{
  "kind": "collectionType",
  "collectionName": "kits",
  "info": {
    "singularName": "kit",
    "pluralName": "kits",
    "displayName": "kit"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "access_notes": {
      "type": "text"
    },
    "level": {
      "type": "integer"
    },
    "is_featured": {
      "type": "boolean",
      "default": false
    },
    "description": {
      "type": "blocks"
    },
    "category_type": {
      "type": "enumeration",
      "enum": [
        "AI",
        "DEVOPS",
        "DOCKER",
        "TESTING",
        "LEARNING",
        "SKILL",
        "PROJECT",
        "WEBAPP",
        "MOBILEAPP",
        "FRONTEND",
        "BACKEND"
      ]
    },
    "experience_level": {
      "type": "enumeration",
      "enum": [
        "COLLEGE",
        "PROFESSIONAL",
        "SCHOOL"
      ]
    },
    "kit_type": {
      "type": "enumeration",
      "default": "FREE",
      "enum": [
        "FREE",
        "PAID",
        "FREEMIUM"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "kit_video": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "youtube_link": {
      "type": "string"
    },
    "kitsubscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::kitsubscription.kitsubscription",
      "mappedBy": "kit"
    },
    "kitlevels": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::kitlevel.kitlevel",
      "mappedBy": "kit"
    },
    "kitprogresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::kitprogress.kitprogress",
      "mappedBy": "kit"
    },
    "orgs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::org.org",
      "inversedBy": "kits"
    },
    "lessons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson.lesson",
      "mappedBy": "kit"
    },
    "resources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resource.resource",
      "mappedBy": "kit"
    },
    "user_repos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-repo.user-repo",
      "mappedBy": "kit"
    },
    "pricing_plans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pricing.pricing",
      "mappedBy": "kit"
    },
    "quizzes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::quiz.quiz",
      "inversedBy": "kits"
    },
    "offers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::offer.offer",
      "mappedBy": "kits"
    }
  }
}
